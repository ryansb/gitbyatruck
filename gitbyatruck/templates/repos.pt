<html metal:use-macro="load: base.pt">
<div metal:fill-slot="head">
  <link rel="stylesheet" href="/static/css/form.css" type="text/css" />
  <!-- JavaScript -->
  <script type="text/javascript"
          src="/static/scripts/jquery-1.4.2.min.js"></script>
  <script type="text/javascript"
          src="/static/scripts/deform.js"></script>
</div>

<div metal:fill-slot="content">
  <div class="row">
    <div class="col-md-2">
      <img class="logo img-responsive" src="${request.static_url('gitbyatruck:static/pyramid.png')}" alt="pyramid web framework">
    </div>
    <div class="col-md-10">
      <div class="content">
        <h1><span class="font-semi-bold">Pyramid</span> <span class="smaller">Alchemy scaffold</span></h1>
        <p class="lead">Welcome to <span class="font-normal">${project}</span>, an&nbsp;application generated&nbsp;by<br>the <span class="font-normal">Pyramid Web Framework 1.5.1</span>.</p>
      </div>
    </div>
  </div>
</div>
</html>
